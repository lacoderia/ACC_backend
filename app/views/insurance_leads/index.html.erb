<h1>Listing insurance_leads</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name f</th>
      <th>Last name m</th>
      <th>Phone number</th>
      <th>Email</th>
      <th>Insurance type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @insurance_leads.each do |insurance_lead| %>
      <tr>
        <td><%= insurance_lead.first_name %></td>
        <td><%= insurance_lead.last_name_f %></td>
        <td><%= insurance_lead.last_name_m %></td>
        <td><%= insurance_lead.phone_number %></td>
        <td><%= insurance_lead.email %></td>
        <td><%= insurance_lead.insurance_type %></td>
        <td><%= link_to 'Show', insurance_lead %></td>
        <td><%= link_to 'Edit', edit_insurance_lead_path(insurance_lead) %></td>
        <td><%= link_to 'Destroy', insurance_lead, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Insurance lead', new_insurance_lead_path %>
